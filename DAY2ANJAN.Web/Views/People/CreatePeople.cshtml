@model CreatePeopleViewModel
<h2>Create People</h2>

<form method="post">
    <div class="mt-3">
        <lable class="form-lable">Name</label>
        <input type="text" class="form-control" asp-for="Name" />
    </div>

    <div class="mt-3">
        <lable class="form-lable">City</label>
        <input type="text" class="form-control" asp-for="City" />
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Create</button>
    </div>
</form>